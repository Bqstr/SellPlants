<?xml version="1.0" encoding="UTF-8"?>


    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
        android:id="@+id/linearShopreviewTwo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylelight_green_800">
            <LinearLayout android:id="@+id/linearColumnvector"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <LinearLayout android:id="@+id/linearRowvectorOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_266pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.71"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700_10x14"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_10x14"/>
                        <ImageView android:id="@+id/imageFrame"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_frame"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_frame"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShopreview"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_79pxh"
                        android:layout_weight="0.95"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_shop_review"
                        style="@style/txtSFProTextregular22" tools:text="@string/lbl_shop_review"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageButton
            android:background="@drawable/rectangle_bg_green_100_radius_42"
            android:id="@+id/btnGroupTwentyOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_84pxh"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="@dimen/_84pxh"
            android:paddingHorizontal="@dimen/_24pxh"
            android:paddingVertical="@dimen/_24pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_group21_gray_54" tools:ignore="ContentDescription"/>
        <TextView android:ellipsize="end" android:id="@+id/txtShop"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_shop"
            style="@style/txtSFProTextsemibold22_2" tools:text="@string/lbl_shop"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtFourHundredFiftyFive"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_4_5_5"
            style="@style/txtSFProTextregular14_3" tools:text="@string/lbl_4_5_5"/>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearRowstar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageStar"
                android:layout_height="@dimen/_19pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star_yellow_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
            <ImageView android:id="@+id/imageStarOne"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star_yellow_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
            <ImageView android:id="@+id/imageStarTwo"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star_yellow_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
            <ImageView android:id="@+id/imageStarThree"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star_yellow_a400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
            <ImageView android:id="@+id/imageStarFour"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star_gray_400"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star_gray_400"/>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListgroupseventythree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_8pxh}" tools:itemCount="3" tools:listitem="@layout/row_listgroupseventythree"/>
    </LinearLayout>
