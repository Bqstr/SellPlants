<?xml version="1.0" encoding="UTF-8"?>


    <LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
        android:id="@+id/linearColumngroupseventythree"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowgroupseventythree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_bg_green_900_44_radius_19"
                android:id="@+id/btnGroupSeventyThree"
                android:layout_height="@dimen/_38pxh"
                android:layout_width="@dimen/_38pxh"
                android:paddingHorizontal="@dimen/_8pxh"
                android:paddingVertical="@dimen/_8pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_group21" tools:ignore="ContentDescription"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnloremipsum"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowloremipsum"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLoremipsum"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_lorem_ipsum"
                        style="@style/txtSFProTextsemibold20" tools:text="@string/lbl_lorem_ipsum"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_48pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.37"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_18_04_1999"
                        style="@style/txtSFProTextregular14" tools:text="@string/lbl_18_04_1999"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowstarFive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStarFive"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarOne"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarTwo"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarThree"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarFour"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_gray_400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_gray_400"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtLanguage"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:text="@string/msg_lorem_ipsum_dol3"
            style="@style/txtSFProTextregular14" tools:text="@string/msg_lorem_ipsum_dol3"/>
    </LinearLayout>

