<?xml version="1.0" encoding="UTF-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent" style="@style/groupStylewhite_A700">

    <FrameLayout

        android:id="@+id/frameStackvector"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_8pxh"
        android:layout_marginStart="@dimen/_9pxh"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout
            android:layout_marginTop="20dp"
            android:layout_marginStart="10dp"
            android:id="@+id/linearColumnvector"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowgogreennurser"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="5dp"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGogreennurser"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.8"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/msg_go_green_nurser"
                    style="@style/txtSFProTextsemibold24" tools:text="@string/msg_go_green_nurser"/>
                <LinearLayout android:id="@+id/linearRowstar"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.2"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStar"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_19pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_black_900"/>
                    <ImageView android:id="@+id/imageVectorThree"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_black_902"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_902"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowemail"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnlocation"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowlocation"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLocation"
                            android:layout_height="@dimen/_18pxh"
                            android:layout_marginBottom="@dimen/_40pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location_gray_600_18x15"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location_gray_600_18x15"/>
                        <TextView android:id="@+id/txtLoremipsuaVolu"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_weight="0.93"
                            android:layout_width="0dp"
                            android:text="@string/msg_lorem_ipsuavolu"
                            style="@style/txtSFProTextregular16_4" tools:text="@string/msg_lorem_ipsuavolu"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvectorFour"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorFour"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_bluegray_400"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_bluegray_400"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_91_99_xx_999_x"
                            style="@style/txtSFProTextregular16_5" tools:text="@string/msg_91_99_xx_999_x"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/msg_gogreen56_gmail"
                        style="@style/txtSFProTextregular16_4" tools:text="@string/msg_gogreen56_gmail"/>
                </LinearLayout>
                <ImageView
                    android:id="@+id/imagePngegg301"
                    android:layout_height="@dimen/_82pxh"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_82pxh"
                    android:scaleType="fitXY"

                    tools:ignore="ContentDescription"
                    tools:src="@drawable/faux_palm_tree"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowpriceOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceOne"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.6"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/msg_delivery_fees"
                    style="@style/txtSFProTextregular16_4" tools:text="@string/msg_delivery_fees"/>
                <LinearLayout android:id="@+id/linearRowstarOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_87pxh"
                    android:layout_weight="0.4"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStarOne"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarTwo"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarThree"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarFour"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_yellow_a400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_yellow_a400"/>
                    <ImageView android:id="@+id/imageStarFive"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_gray_400"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_gray_400"/>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowgrouptwentythree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <EditText
                    android:background="@drawable/rectangle_bg_gray_50_radius_9"
                    android:drawablePadding="@dimen/_10pxh"
                    android:drawableStart="@drawable/img_search"
                    android:ellipsize="end"
                    android:hint="@string/lbl_search_here"
                    android:id="@+id/etGroupTwentyThree"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.85"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:textColorHint="@color/gray_500"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <ImageButton
                    android:background="@drawable/rectangle_bg_light_green_800_radius_9"
                    android:id="@+id/btnGroupTwelve"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_width="@dimen/_48pxh"
                    android:paddingHorizontal="@dimen/_14pxh"
                    android:paddingVertical="@dimen/_14pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group12" tools:ignore="ContentDescription"/>


            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView

                android:id="@+id/recyclerrr"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

        </LinearLayout>

    </FrameLayout>
    </ScrollView>

