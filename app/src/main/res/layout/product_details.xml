<?xml version="1.0" encoding="utf-8"?>
<LinearLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_layout"
    android:layout_gravity="bottom|start"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_10pxv"
    android:layout_width="match_parent"
    android:orientation="horizontal" style="@style/groupStylewhite_1">
    <ImageView android:id="@+id/imageArrowleft"
        android:layout_height="25dp"
        android:layout_marginBottom="@dimen/_344pxv"
        android:layout_marginStart="@dimen/_27pxh"
        android:layout_marginTop="@dimen/_26pxv"
        android:layout_width="16dp"
        android:scaleType="fitXY"
        android:src="@drawable/img_arrowleft_light_green_800"
        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_light_green_800"/>
    <FrameLayout android:id="@+id/frameStackedit"
        android:layout_height="@dimen/_319pxv"
        android:layout_marginBottom="@dimen/_46pxv"
        android:layout_marginStart="@dimen/_5pxh"
        android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_294pxh">
        <ImageView android:id="@+id/imageEdit"
            android:layout_gravity="top|end"
            android:layout_height="@dimen/_19pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_19pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_edit"
            tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
        <LinearLayout
            android:id="@+id/linearColumngroupsixtythree"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtGroupSixtyThree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_59pxh"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_2pxv"
                android:paddingStart="@dimen/_22pxh"
                android:paddingTop="@dimen/_2pxv"
                android:singleLine="true"
                android:text="@string/lbl_product_details"
                style="@style/txtSolid" tools:text="@string/lbl_product_details"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrectangletwelveFour"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_60pxh"
                android:layout_marginStart="@dimen/_60pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                <ImageView
                    android:gravity="center_horizontal"
                    android:id="@+id/imageRectangleTwelveFour"
                    android:layout_height="@dimen/_148pxh"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"

                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle12"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMonsteraplantsTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@string/lbl_monstera_plants"
                    style="@style/txtSFProTextregular20" tools:text="@string/lbl_monstera_plants"/>
            </LinearLayout>
            <TextView
                android:gravity="center_horizontal"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_lorem_ipsum_dol"
                style="@style/txtSFProTextregular14_2" tools:text="@string/msg_lorem_ipsum_dol"/>
            <LinearLayout
                android:id="@+id/linearRowqtyeight"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtQtyEight"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.44"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/lbl_qty_8"
                    style="@style/txtSFProTextregular17" tools:text="@string/lbl_qty_8"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceFour"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_168pxh"
                    android:layout_weight="0.56"
                    android:layout_width="0dp"
                    android:singleLine="true"
                    android:text="@string/lbl_399"
                    style="@style/txtSFProTextsemibold24" tools:text="@string/lbl_399"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
    <ImageView android:id="@+id/imageDelete"
        android:layout_height="@dimen/_20pxh"
        android:layout_marginBottom="@dimen/_342pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_marginTop="@dimen/_21pxv"
        android:layout_width="@dimen/_18pxh"
        android:scaleType="fitXY"
        android:src="@drawable/img_delete"
        tools:ignore="ContentDescription" tools:src="@drawable/img_delete"/>
</LinearLayout>