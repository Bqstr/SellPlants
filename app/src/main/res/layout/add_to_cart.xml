<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:id="@+id/linearColumnrectangletwelveFour"
        style="@style/groupStylewhite_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageRectangleTwelveFour"
            android:layout_width="@dimen/_110pxh"
            android:layout_height="@dimen/_108pxh"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:scaleType="fitXY"
            android:src="@drawable/faux_palm_tree" />

        <LinearLayout
            android:id="@+id/linearColumnmonsteraplantsFour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginBottom="@dimen/_61pxv"
            android:gravity="start"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtMonsteraplantsFour"
                style="@style/txtSFProTextregular20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_71pxh"
                android:layout_marginEnd="@dimen/_68pxh"
                android:ellipsize="end"
                android:gravity="center_horizontal"
                android:singleLine="true"
                android:text="@string/lbl_monstera_plants" />

            <TextView
                android:id="@+id/txtLanguage"
                style="@style/txtSFProTextregular14_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                android:text="@string/msg_lorem_ipsum_dol" />

            <LinearLayout
                android:id="@+id/linearRowpriceEight"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtPriceEight"
                    style="@style/txtSFProTextsemibold18_6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.32"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_399" />

                <LinearLayout
                    android:id="@+id/linearRowlanguageOne"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_126pxh"
                    android:layout_weight="0.68"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtLanguageOne"
                        style="@style/txtSFProTextsemibold18_6"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.51"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_total" />

                    <TextView
                        android:id="@+id/txtPriceNine"
                        style="@style/txtSFProTextsemibold18_6"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.49"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_399" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowvectorTwo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_71pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_71pxh"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVectorTwo"
                    android:layout_width="19dp"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_red_500" />

                <TextView
                    android:id="@+id/txtOne"
                    style="@style/txtSFProTextsemibold20_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_weight="0.22"
                    android:ellipsize="end"

                    android:text="@string/lbl_1" />

                <ImageView
                    android:id="@+id/imageVectorThree"
                    android:layout_width="19dp"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_vector_green_900"
                    tools:ignore="ContentDescription" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton

        android:id="@+id/btnAddToCart"
        app:layout_constraintTop_toBottomOf="@+id/linearColumnrectangletwelveFour"
        style="@style/btnSolidRoundedBottomCorner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_12pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_12pxv"
        android:singleLine="true"
        android:text="@string/lbl_add_to_cart"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="@+id/linearColumnrectangletwelveFour"
        tools:layout_editor_absoluteX="11dp" />


</androidx.constraintlayout.widget.ConstraintLayout>