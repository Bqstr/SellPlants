<?xml version="1.0" encoding="UTF-8"?>

    <LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:gravity="center_horizontal"
        android:id="@+id/linearUsereditProfil"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylelight_green_800">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnvector"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                    <LinearLayout android:id="@+id/linearRowvectorOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginStart="@dimen/_266pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.71"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectorOne"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700"/>
                        <ImageView android:id="@+id/imageVectorTwo"
                            android:layout_height="@dimen/_10pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_width="@dimen/_14pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_white_a700_10x14"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_white_a700_10x14"/>
                        <ImageView android:id="@+id/imageFrame"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_21pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_frame"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_frame"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEditprofile"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_90pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_edit_profile"
                        style="@style/txtSFProTextregular22" tools:text="@string/lbl_edit_profile"/>
                    <ImageView android:id="@+id/imageLocation"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_78pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ImageButton
            android:background="@drawable/rectangle_bg_green_900_44_radius_43"
            android:id="@+id/btnGroupTwentyOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_86pxh"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_86pxh"
            android:paddingHorizontal="@dimen/_19pxh"
            android:paddingVertical="@dimen/_19pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_group21" tools:ignore="ContentDescription"/>
        <androidx.appcompat.widget.SearchView
            android:background="@drawable/rectangle_border_gray_600_radius_6"
            android:gravity="center_horizontal"
            android:id="@+id/searchViewGroupEleven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:theme="@style/searchViewGroupEleven"
            app:iconifiedByDefault="false"
            app:queryBackground="@android:color/transparent"
            app:queryHint="@string/lbl_ravi_jagani" app:searchIcon="@drawable/img_search_gray_600"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_600_radius_6"
            android:drawablePadding="@dimen/_14pxh"
            android:drawableStart="@drawable/img_stroke_1"
            android:ellipsize="end"
            android:hint="@string/msg_91_99_xx_999_x"
            android:id="@+id/etPrice" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:textColorHint="@color/gray_600"
            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowcountry"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <EditText
                android:background="@drawable/rectangle_border_gray_600_radius_6"
                android:drawablePadding="@dimen/_11pxh"
                android:drawableStart="@drawable/img_location_gray_600"
                android:ellipsize="end" android:hint="@string/lbl_india"
                android:id="@+id/etCountry" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.42" android:layout_width="0dp"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:textColorHint="@color/gray_600"
                style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnGujrat"
                android:layout_height="@dimen/_49pxv"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_91pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_gujrat"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline" tools:text="@string/lbl_gujrat"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnRajkot"
                android:layout_height="@dimen/_49pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_width="@dimen/_75pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_11pxh"
                android:paddingStart="@dimen/_11pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_rajkot"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline" tools:text="@string/lbl_rajkot"/>
        </LinearLayout>
        <EditText
            android:background="@drawable/rectangle_border_gray_600_radius_6"
            android:drawablePadding="@dimen/_17pxh"
            android:drawableStart="@drawable/img_location_gray_600"
            android:ellipsize="end"
            android:hint="@string/msg_near_a_iscon_te"
            android:id="@+id/etGroupThirteen" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_22pxh"
            android:paddingTop="@dimen/_13pxv" android:singleLine="true"
            android:textColorHint="@color/gray_600"
            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnUpdate"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_49pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_33pxv"
            android:layout_width="@dimen/_245pxh"
            android:paddingBottom="@dimen/_11pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_11pxv" android:singleLine="true"
            android:text="@string/lbl_update"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_update"/>
    </LinearLayout>

