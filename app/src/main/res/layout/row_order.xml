<?xml version="1.0" encoding="UTF-8"?>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/linearRowmonsteraplants"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylelight_green_800_66">
        <TextView android:ellipsize="end"
            android:id="@+id/txtMonsteraplants"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_18pxv"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_weight="0.67" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@string/msg_monstera_plants"
            style="@style/txtSFProTextregular18_3" tools:text="@string/msg_monstera_plants"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPrice"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_weight="0.22" android:layout_width="0dp"
            android:singleLine="true" android:text="@string/lbl_1299"
            style="@style/txtSFProTextregular18_3" tools:text="@string/lbl_1299"/>
        <ImageButton
            android:background="@drawable/rectangle_bg_red_500_93_border_deep_orange_a700_radius_13_5"
            android:id="@+id/btnGroup112"
            android:layout_height="@dimen/_27pxh"
            android:layout_marginBottom="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_27pxh"
            android:paddingHorizontal="@dimen/_7pxh"
            android:paddingVertical="@dimen/_7pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_group112" tools:ignore="ContentDescription"/>
    </LinearLayout>
